  "source": "pragma solidity ^0.4.22;\n\ncontract Dummy{\n    uint256 test;\n\n    event Mined(uint256 indexed value);\n    event Hello(address sender);\n\n    // this function is in order to produce blocks.\n    function mine() public returns(uint256){\n        test += 1;\n        emit Hello(msg.sender);\n        emit Mined(test);\n        return test;\n    }\n}",
